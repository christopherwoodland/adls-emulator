{
  "name": "adls-emulator",
  "version": "0.1.0",
  "description": "Azure Data Lake Storage Gen2 Emulator for local development",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --detectOpenHandles",
    "examples": "node examples.js",
    "docker:build": "docker build -t adls-emulator:latest .",
    "docker:run": "docker run -p 10000:10000 -v adls-data:/app/data adls-emulator:latest"
  },
  "keywords": [
    "azure",
    "data-lake-storage",
    "gen2",
    "emulator",
    "blob-storage"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "uuid": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
